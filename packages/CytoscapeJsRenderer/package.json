{
  "name": "@cytoscape/cytoscapejs-renderer",
  "author": "Keiichiro Ono <kono@ucsd.edu>",
  "version": "v0.2.0",
  "description": "Network renderer components for CyNetworkViewer. Current version includes Cytoscape.js renderer and Sigma.js renderer.",
  "license": "MIT",
  "keywords": [
    "cytoscape",
    "visualization",
    "graph",
    "bioinformatics"
  ],
  "main": "build/CytoscapeJsRenderer.js",
  "files": [
    "webpack.config.babel.js",
    ".preprocessor.js",
    ".travis.yml",
    ".babelrc",
    "build/*",
    "src/*"
  ],
  "scripts": {
    "build": "./node_modules/.bin/webpack --mode development --progress --colors",
    "clean": "rm build/*",
    "lint": "eslint src/; exit 0",
    "test": "eslint src/ && jest",
    "coverage": "jest --coverage"
  },
  "homepage": "https://github.com/CyComponent/CytoscapeJsRenderer",
  "repository": {
    "type": "git",
    "url": "https://github.com/CyComponent/CytoscapeJsRenderer.git"
  },
  "bugs": {
    "url": "https://github.com/CyComponent/CytoscapeJsRenderer/issues"
  },
  "dependencies": {
    "cytoscape": "^3.5.2",
    "cytoscape-cose-bilkent": "^4.0.0",
    "eslint-config-prettier": "^6.3.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.14.3",
    "immutable": "^4.0.0-rc.12",
    "sigma": "^1.2.1"
  },
  "peerDependencies": {
    "react": "16.8.6"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-do-expressions": "^7.0.0",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-logical-assignment-operators": "^7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-optional-chaining": "^7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^9.0.0",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.0",
    "eslint": "^5.16.0",
    "jest": "^24.7.1",
    "prettier": "^1.16.4",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0"
  },
  "jest": {
    "scriptPreprocessor": "<rootDir>/.preprocessor.js"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": "eslint:recommended",
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "env": {
      "es6": true,
      "browser": true
    }
  }
}
